module compare4 (
   input alufn[6],
   input z,
   input v,
   input n,
   output out[32]  //only using LSB bit
  ) {

  always {
    out[31:1] = 0;

    case(alufn[2:1]) {
    2b01:
      out[0] = z;
    2b10:
      out[0] = n ^ v;
    2b11:
      out[0] = z | (n ^ v);
    default:
      out[0] = 0;
    }
  }
}
